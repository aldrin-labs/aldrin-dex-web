{
  "name": "web",
  "repository": "git@gitlab.com:crypto_project/frontend/web.git",
  "author": "CCAI-team",
  "version": "0.0.1",
  "sideEffects": [
    "*.woff2",
    "app.styles.global.css",
    "*.css"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 webpack-dev-server --env env=dev",
    "dev:fast": "cross-env NODE_ENV=development LOCAL_BUILD=true NODE_OPTIONS=--max_old_space_size=4096 DEVTOOL=eval-cheap-module-source-map webpack-dev-server --env env=dev",
    "dev:fast:local": "cross-env NODE_ENV=development LOCAL_BUILD=true LOCAL_BACKEND_BUILD=true NODE_OPTIONS=--max_old_space_size=4096 DEVTOOL=eval-cheap-module-source-map  webpack-dev-server --env env=dev",
    "dev:bundleanalyzer": "yarn dev --env addons=bundleanalyzer",
    "prebuild": "rimraf dist",
    "build:stats": "yarn build --json --progress --profile > webpack-build-log.json",
    "submodule": "git submodule sync && git submodule init && git submodule update",
    "build:without:submodule": "cross-env DEVTOOL=cheap-source-map NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production webpack --mode production --env env=prod",
    "build": "npm run submodule && cross-env  NODE_ENV=production DEVTOOL=cheap-source-map NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production webpack --mode production --env env=prod",
    "buildMaster": "npm run submodule && cross-env NODE_ENV=production NODE_ENV=production MASTER_BUILD=TRUE webpack --mode production --env env=prod",
    "build:bundleanalyzer:without:submodule": "yarn NODE_ENV=production build:without:submodule --env addons=bundleanalyzer",
    "build:bundleanalyzer": "yarn build NODE_ENV=production --env addons=bundleanalyzer",
    "start:ci": "serve -l 3000",
    "test": "cross-env NODE_ENV=test jest",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:ci": "cypress run --browser chrome",
    "typecheck": "tsc",
    "lint:css": "stylelint './src/**/*.tsx'",
    "clear": "rm -rf node_modules/ && rm -rf yarn.lock && yarn",
    "fetch-local-core": "rsync -azP --exclude=node_modules ../core ./src",
    "fetch-local-storybook": "rsync -azP --exclude=node_modules ../storybook ./src",
    "lint:ts": "eslint --ext .js,.jsx,.ts,.tsx src",
    "prepare": "husky install"
  },
