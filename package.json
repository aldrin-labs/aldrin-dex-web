{
  "name": "web",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@gitlab.com:crypto_project/frontend/web.git",
  "author": "Bogdan Volyk <dekultism@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel ./app/index.html",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/app.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "dependencies": {
    "polished": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "jest-cli": "^22.0.4",
    "parcel-bundler": "^1.3.1",
    "prettier": "^1.9.2"
  }
}
